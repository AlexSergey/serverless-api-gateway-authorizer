  "main": "index.js",
  "scripts": {
    "debug": "SET SLS_DEBUG=* && node --inspect node_modules\\serverless\\bin\\serverless.js offline -s dev",
    "deploy": "serverless deploy",
    "format": "npm run format:package && npm run format:prettier && npm run format:code",
    "format:code": "eslint --ext .ts,.json src/ tests/ --fix",
    "format:package": "sort-package-json",
    "format:prettier": "prettier --write \"src/**/*.{ts,json}\" \"tests/**/*.{ts,json}\"",
    "lint": "npm run lint:ts && npm run lint:code",
    "lint:code": "eslint --ext .ts,.json src/ tests/",
    "lint:commit": "commitlint -e",
    "lint:ts": "tsc --noEmit",
    "package": "serverless package",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "start": "serverless offline start -r us-east-1 --noTimeout",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "@middy/core": "^3.6.2",
    "@middy/http-error-handler": "^3.6.2",
